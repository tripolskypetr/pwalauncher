{
  "name": "pwalauncher",
  "version": "1.0.6",
  "description": "The PWA launcher for react-declarative",
  "main": "index.mjs",
  "bin": "index.mjs",
  "scripts": {
    "start": "node index.mjs",
    "create-ssl-ip": "sh ./scripts/create-ssl-ip.sh",
    "create-ssl-host": "sh ./scripts/create-ssl-host.sh",
    "test": "node --inspect-brk index.mjs"
  },
  "keywords": [
    "react-declarative",
    "pwa",
    "reverse-proxy",
    "https",
    "http",
    "port-forwarding",
    "path-proxy",
    "cors",
    "mixed-content",
    "same-origin",
    "websocket"
  ],
